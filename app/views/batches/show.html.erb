<%# <div>  left side%>
<h2> Batch: <%= @batch.name %> </h2>
<h2> Start Date:  <%= @batch.start_date %> </h2>
<h2> End Date:  <%= @batch.end_date %> </h2>
<details>
  <summary> Lesson List: <%= @batch.lessons.count %> </summary>
  <% @batch.lessons.each do |lesson| %>
    <p> <%= lesson.title %> </p>
  <% end %>
</details>
<%# <div> right side  %>
<h2> Student List </h2>
<%= @batch.students.inspect %>
<% @batch.students.each do |student| %>
<p> <%= student.first_name %> <%= student.last_name %>, Progress </p>
<% end %>